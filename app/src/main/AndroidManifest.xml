<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.acterics.sandbox">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/SandBoxTheme">
        <activity
            android:name=".ActivityOne"
            android:theme="@style/SandBoxTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".webstore.ActivityMainWebStore"
            android:theme="@style/WebStoreTheme" />
        <activity
            android:name=".webstore.database.ActivityStoreDatabase"
            android:theme="@style/WebStoreTheme" />
        <activity
            android:name=".webstore.database.DialogEntryInput"
            android:theme="@style/EntryInputTheme"></activity>
        <activity android:name=".chat.ActivityMainChat"></activity>
    </application>

</manifest>